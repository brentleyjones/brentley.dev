{
  "name": "brentley.dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/static/css/tailwind.css --o _site/static/css/style.css",
    "clean": "rimraf _site",
    "dev": "concurrently npm:watch:*",
    "watch:browser": "browser-sync start --no-open --no-notify --port 8080 --server '_site' --files '_site'",
    "watch:css": "postcss src/static/css/tailwind.css --o _site/static/css/style.css --watch",
    "watch:eleventy": "cross-env ELEVENTY_ENV=development eleventy --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@tailwindcss/typography": "^0.3.1",
    "alpinejs": "^2.6.0",
    "autoprefixer": "^10.1.0",
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eleventy-plugin-embed-twitter": "^1.1.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "luxon": "^1.25.0",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-prism": "^2.1.3",
    "markdown-it-replace-link": "^1.1.0",
    "minify-xml": "^2.4.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "prism-themes": "^1.5.0",
    "prismjs": "^1.21.0",
    "rimraf": "^2.7.1",
    "tailwindcss": "^2.0.2"
  }
}
